# Xác định phiên bản iOS tối thiểu mà ứng dụng hỗ trợ
platform :ios, '13.0'
use_frameworks!

# Tên của mục tiêu (target) cần khai báo
target 'MyApp' do
  # Thêm các pod (thư viện) mà bạn muốn sử dụng trong dự án của mình
  pod 'Alamofire', '~> 5.4'  # Ví dụ: Thư viện giúp thực hiện các yêu cầu HTTP
  pod 'Firebase/Analytics'   # Ví dụ: Thư viện của Firebase để phân tích dữ liệu
  pod 'SDWebImage', '~> 5.0' # Ví dụ: Thư viện để tải và lưu trữ ảnh từ URL

  # Thêm các thư viện khác nếu cần
end
